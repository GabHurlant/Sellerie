{% extends 'base.html.twig' %}

{% block title %}Log in{% endblock %}

{% block body %}
<h1>Log in</h1>

{% if error %}
    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
{% endif %}
<div class="pt-16">
{{ form_start(form) }}
    {{ form_row(form.mail) }}
    {{ form_row(form.password) }}
    {{ form_row(form.submit) }}
{{ form_end(form) }}
</div>
{% endblock %}